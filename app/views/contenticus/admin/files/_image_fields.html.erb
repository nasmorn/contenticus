<div class="well imageform">
  <%= form.fields_for "field_#{name}",  OpenStruct.new(form.object.send("field_#{name}")) do |image_form| %>
    Image id: <%= image_form.text_field_without_bootstrap :file_id %> <br>
    Crop <%= image_form.text_field_without_bootstrap :crop_x, class: "crop_x" %> x <%= image_form.text_field_without_bootstrap :crop_y, class: "crop_y" %>
    x <%= image_form.text_field_without_bootstrap :crop_h, class: "crop_h" %>
    x <%= image_form.text_field_without_bootstrap :crop_w, class: "crop_w" %>

    <% if Contenticus::File.exists?(image_form.object[:file_id]) %>
      <%= image_tag Contenticus::File.find(image_form.object[:file_id]).file.url, class: "jcrop" %>
    <% end %>
  <% end %>
</div>